{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "buildCommand": "cd frontend && npm install && npm run build"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/frontend/dist/$1" }
    ]
  }